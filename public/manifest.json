{
  "manifest_version": 2,
  "name": "BrowsingLLaMa",
  "version": "0.0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["activeTab", "scripting"],
  "options_ui": {
    "page": "options.html"
  }
}
